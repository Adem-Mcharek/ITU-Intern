/* 
WebTV Processor - Professional Design
Inspired by UNWTO and UN agencies with clean, modern aesthetics
Professional typography, subtle shadows, and generous whitespace
*/

/* CSS Variables for Design System */
:root {
    /* Colors - Professional palette with better contrast */
    --itu-blue: #009EDB;
    --itu-blue-dark: #0077B6;
    --itu-blue-light: #F0F8FF;
    --itu-blue-hover: #0088CC;
    
    --white: #FFFFFF;
    --background: #FAFBFC;
    --background-light: #F8F9FA;
    --text-primary: #000000;
    --text-secondary: #000000;
    --text-muted: #000000;
    --border-light: #E2E8F0;
    --border-medium: #CBD5E0;
    
    --success: #38A169;
    --warning: #D69E2E;
    --danger: #E53E3E;
    --info: var(--itu-blue);
    
    /* Typography - Professional scale */
    --font-family-primary: 'Inter', 'Segoe UI', system-ui, -apple-system, sans-serif;
    --font-weight-normal: 400;
    --font-weight-medium: 500;
    --font-weight-semibold: 600;
    --font-weight-bold: 700;
    
    /* Spacing - Generous whitespace */
    --spacing-xs: 0.25rem;
    --spacing-sm: 0.5rem;
    --spacing-md: 1rem;
    --spacing-lg: 1.5rem;
    --spacing-xl: 2rem;
    --spacing-xxl: 3rem;
    --spacing-xxxl: 4rem;
    
    /* Shadows - Subtle and professional */
    --shadow-sm: 0 1px 3px rgba(0, 0, 0, 0.06);
    --shadow-md: 0 4px 12px rgba(0, 0, 0, 0.08);
    --shadow-lg: 0 8px 25px rgba(0, 0, 0, 0.10);
    
    /* Border Radius - Refined */
    --radius-sm: 6px;
    --radius-md: 8px;
    --radius-lg: 12px;
    --radius-xl: 16px;
}

/* Base Styles */
* {
    box-sizing: border-box;
}

body {
    font-family: var(--font-family-primary);
    font-weight: var(--font-weight-normal);
    line-height: 1.7;
    color: #000000 !important;
    background-color: var(--background-light);
    margin: 0;
    padding: 0;
    display: flex;
    flex-direction: column;
    min-height: 100vh;
    font-size: 16px;
}

/* Target specific text elements that should be black for readability */
body, p, div:not(.btn):not(.alert):not(.badge), span:not(.badge), 
h1, h2, h3, h4, h5, h6, li, td, th, label, input, textarea, select {
    color: #000000 !important;
}

/* Keep original colors for interactive elements */
.alert, .alert *, .badge, .badge * {
    color: initial !important;
}

/* Button colors - explicit to ensure visibility */
.btn-primary, .btn-primary * {
    color: var(--white) !important;
}

.btn-outline-primary, .btn-outline-primary * {
    border: 2px solid var(--itu-blue) !important;
    color: var(--white) !important;
    background-color: var(--itu-blue) !important;
}

.btn-outline-primary:hover {
    background-color: var(--itu-blue-hover) !important;
    color: var(--white) !important;
    transform: translateY(-1px);
    box-shadow: var(--shadow-md);
    border-color: var(--itu-blue-hover) !important;
}

.btn-outline-secondary, .btn-outline-secondary * {
    border: 2px solid var(--text-secondary) !important;
    color: var(--white) !important;
    background-color: var(--text-secondary) !important;
}

.btn-outline-secondary:hover {
    background-color: #000000 !important;
    color: var(--white) !important;
    border-color: #000000 !important;
}

.btn-success, .btn-success * {
    color: var(--white) !important;
}

.btn-outline-success, .btn-outline-success * {
    border: 2px solid var(--success) !important;
    color: var(--white) !important;
    background-color: var(--success) !important;
}

.btn-outline-success:hover {
    background-color: #2F855A !important;
    color: var(--white) !important;
    border-color: #2F855A !important;
}

.btn-warning, .btn-warning * {
    color: var(--text-primary) !important;
}

.btn-danger, .btn-danger * {
    color: var(--white) !important;
}

.btn-outline-danger, .btn-outline-danger * {
    border: 2px solid var(--danger) !important;
    color: var(--white) !important;
    background-color: var(--danger) !important;
}

.btn-outline-danger:hover {
    background-color: #C53030 !important;
    color: var(--white) !important;
    border-color: #C53030 !important;
}

.btn-info, .btn-info * {
    color: var(--white) !important;
}

.btn-close {
    color: var(--text-secondary) !important;
}

/* Specific overrides for common text utility classes */
.text-muted:not(.btn):not(.alert):not(.badge), 
.text-secondary:not(.btn):not(.alert):not(.badge), 
.text-primary:not(.btn):not(.alert):not(.badge) {
    color: #000000 !important;
}

/* Typography - Clean and professional */
h1, h2, h3, h4, h5, h6 {
    font-weight: var(--font-weight-semibold);
    line-height: 1.25;
    margin-bottom: var(--spacing-lg);
    color: var(--text-primary);
    letter-spacing: -0.025em;
}

h1 { font-size: 2.5rem; }
h2 { font-size: 2rem; }
h3 { font-size: 1.5rem; }
h4 { font-size: 1.25rem; }

.display-1, .display-2, .display-3, .display-4 {
    font-weight: var(--font-weight-bold);
    line-height: 1.1;
    letter-spacing: -0.02em;
}

.lead {
    font-size: 1.125rem;
    font-weight: var(--font-weight-normal);
    color: var(--text-primary);
    line-height: 1.6;
    margin-bottom: var(--spacing-xl);
}

p {
    margin-bottom: var(--spacing-lg);
    color: #000000 !important;
}

/* Layout - Generous spacing */
main {
    flex: 1;
    padding-top: var(--spacing-xxxl);
    padding-bottom: var(--spacing-xxxl);
}

.container-xl {
    max-width: 1200px;
    margin: 0 auto;
    padding-left: var(--spacing-xl);
    padding-right: var(--spacing-xl);
}

/* Navigation - Clean and professional */
.navbar {
    background-color: var(--white) !important;
    border-bottom: 1px solid var(--border-light);
    box-shadow: var(--shadow-sm);
    position: sticky;
    top: 0;
    z-index: 1030;
    padding-top: var(--spacing-lg);
    padding-bottom: var(--spacing-lg);
}

.navbar-brand {
    font-weight: var(--font-weight-bold);
    font-size: 1.5rem;
    color: var(--itu-blue) !important;
    display: flex;
    align-items: center;
    gap: var(--spacing-md);
    text-decoration: none;
}

.navbar-brand:hover {
    color: var(--itu-blue-hover) !important;
}

.navbar-brand img {
    height: 40px;
    width: auto;
}

/* Navigation links should be black for visibility */
.nav-link {
    font-weight: var(--font-weight-medium);
    color: #000000 !important;
    padding: var(--spacing-md) var(--spacing-lg) !important;
    border-radius: var(--radius-md);
    transition: all 0.2s ease;
    font-size: 0.95rem;
}

.nav-link:hover,
.nav-link.active {
    color: var(--itu-blue) !important;
    background-color: var(--itu-blue-light);
}

/* Buttons - Professional styling */
.btn {
    border-radius: var(--radius-md) !important;
    font-weight: var(--font-weight-medium);
    letter-spacing: 0.025em;
    transition: all 0.2s ease;
    border-width: 2px !important;
    border-style: solid !important;
    padding: var(--spacing-sm) var(--spacing-lg);
    text-decoration: none;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    gap: var(--spacing-xs);
    line-height: 1.5;
    position: relative;
    overflow: hidden;
}

.btn-primary {
    background-color: var(--itu-blue) !important;
    color: var(--white) !important;
    border-color: var(--itu-blue) !important;
}

.btn-primary:hover {
    background-color: var(--itu-blue-hover) !important;
    color: var(--white) !important;
    border-color: var(--itu-blue-hover) !important;
    transform: translateY(-1px);
    box-shadow: var(--shadow-md);
}

.btn-outline-primary {
    background-color: transparent !important;
    color: var(--itu-blue) !important;
    border-color: var(--itu-blue) !important;
}

.btn-outline-primary:hover {
