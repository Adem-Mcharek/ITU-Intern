    font-weight: var(--font-weight-semibold);
    line-height: 1.25;
    margin-bottom: var(--spacing-lg);
    color: var(--text-primary);
    letter-spacing: -0.025em;
}

h1 { font-size: 2.5rem; }
h2 { font-size: 2rem; }
h3 { font-size: 1.5rem; }
h4 { font-size: 1.25rem; }

.display-1, .display-2, .display-3, .display-4 {
    font-weight: var(--font-weight-bold);
    line-height: 1.1;
    letter-spacing: -0.02em;
}

.lead {
    font-size: 1.125rem;
    font-weight: var(--font-weight-normal);
    color: var(--text-primary);
    line-height: 1.6;
    margin-bottom: var(--spacing-xl);
}

p {
    margin-bottom: var(--spacing-lg);
    color: #000000 !important;
}

/* Layout - Generous spacing */
main {
    flex: 1;
    padding-top: var(--spacing-xxxl);
    padding-bottom: var(--spacing-xxxl);
}

.container-xl {
    max-width: 1200px;
    margin: 0 auto;
    padding-left: var(--spacing-xl);
    padding-right: var(--spacing-xl);
}

/* Navigation - Clean and professional */
.navbar {
    background-color: var(--white) !important;
    border-bottom: 1px solid var(--border-light);
    box-shadow: var(--shadow-sm);
    position: sticky;
    top: 0;
    z-index: 1030;
    padding-top: var(--spacing-lg);
    padding-bottom: var(--spacing-lg);
}

.navbar-brand {
    font-weight: var(--font-weight-bold);
    font-size: 1.5rem;
    color: var(--itu-blue) !important;
    display: flex;
    align-items: center;
    gap: var(--spacing-md);
    text-decoration: none;
}

.navbar-brand:hover {
    color: var(--itu-blue-hover) !important;
}

.navbar-brand img {
    height: 40px;
    width: auto;
}

/* Navigation links should be black for visibility */
.nav-link {
    font-weight: var(--font-weight-medium);
    color: #000000 !important;
    padding: var(--spacing-md) var(--spacing-lg) !important;
    border-radius: var(--radius-md);
    transition: all 0.2s ease;
    font-size: 0.95rem;
}

.nav-link:hover,
.nav-link.active {
    color: var(--itu-blue) !important;
    background-color: var(--itu-blue-light);
}

/* Buttons - Professional styling */
.btn {
    border-radius: var(--radius-md) !important;
    font-weight: var(--font-weight-medium);
    letter-spacing: 0.025em;
    transition: all 0.2s ease;
    border-width: 2px !important;
    border-style: solid !important;
    padding: var(--spacing-sm) var(--spacing-lg);
    text-decoration: none;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    gap: var(--spacing-xs);
    line-height: 1.5;
    position: relative;
    overflow: hidden;
}

.btn-primary {
    background-color: var(--itu-blue) !important;
    color: var(--white) !important;
    border-color: var(--itu-blue) !important;
}

.btn-primary:hover {
    background-color: var(--itu-blue-hover) !important;
    color: var(--white) !important;
    border-color: var(--itu-blue-hover) !important;
    transform: translateY(-1px);
    box-shadow: var(--shadow-md);
}

.btn-outline-primary {
    background-color: transparent !important;
    color: var(--itu-blue) !important;
    border-color: var(--itu-blue) !important;
}

.btn-outline-primary:hover {
    background-color: var(--itu-blue) !important;
    color: var(--white) !important;
    border-color: var(--itu-blue) !important;
    transform: translateY(-1px);
    box-shadow: var(--shadow-md);
}

.btn-outline-secondary {
    background-color: transparent !important;
    color: var(--text-secondary) !important;
    border-color: var(--border-medium) !important;
}

.btn-outline-secondary:hover {
    background-color: var(--text-secondary) !important;
    color: var(--white) !important;
    border-color: var(--text-secondary) !important;
    transform: translateY(-1px);
    box-shadow: var(--shadow-md);
}

.btn-success {
    background-color: var(--success) !important;
    color: var(--white) !important;
    border-color: var(--success) !important;
}

.btn-outline-success {
    background-color: transparent !important;
    color: var(--success) !important;
    border-color: var(--success) !important;
}

.btn-outline-success:hover {
    background-color: var(--success) !important;
    color: var(--white) !important;
    border-color: var(--success) !important;
    transform: translateY(-1px);
    box-shadow: var(--shadow-md);
}

.btn-warning {
    background-color: var(--warning) !important;
    color: var(--text-primary) !important;
    border-color: var(--warning) !important;
}

.btn-danger {
    background-color: var(--danger) !important;
    color: var(--white) !important;
    border-color: var(--danger) !important;
}

.btn-outline-danger {
    background-color: transparent !important;
    color: var(--danger) !important;
    border-color: var(--danger) !important;
}

.btn-outline-danger:hover {
    background-color: var(--danger) !important;
    color: var(--white) !important;
    border-color: var(--danger) !important;
    transform: translateY(-1px);
    box-shadow: var(--shadow-md);
}

.btn-info {
    background-color: var(--info) !important;
    color: var(--white) !important;
    border-color: var(--info) !important;
}

.btn-close {
    color: var(--text-secondary) !important;
}

/* Specific overrides for common text utility classes */
.text-muted:not(.btn):not(.alert):not(.badge), 
.text-secondary:not(.btn):not(.alert):not(.badge), 
.text-primary:not(.btn):not(.alert):not(.badge) {
    color: #000000 !important;
